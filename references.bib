	file = {Bertoni et al. - 2021 - Bioactivity descriptors for uncharacterized chemic.pdf:/home/acomajuncosa/snap/zotero-snap/common/Zotero/storage/TWSG749P/s41467-021-24150-4 (12).pdf:application/pdf},
