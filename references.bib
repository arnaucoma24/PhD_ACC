	pages = {2031--2043},
	file = {Wood et al. - 2012 - Pharmacophore Fingerprint-Based Approach to Bindin.pdf:/home/acomajuncosa/snap/zotero-snap/common/Zotero/storage/5XXRISAU/Wood et al. - 2012 - Pharmacophore Fingerprint-Based Approach to Bindin.pdf:application/pdf},
