	pages = {3049--3062},
	file = {Sun et al. - 2022 - Why 90% of clinical drug development fails and how.pdf:/home/acomajuncosa/snap/zotero-snap/common/Zotero/storage/3NZXXDUU/Sun et al. - 2022 - Why 90% of clinical drug development fails and how.pdf:application/pdf},
}

