	pages = {1880--1887},
	file = {Ezkurdia et al. - 2015 - Most Highly Expressed Protein-Coding Genes Have a .pdf:/home/acomajuncosa/snap/zotero-snap/common/Zotero/storage/J4IPSXFH/Ezkurdia et al. - 2015 - Most Highly Expressed Protein-Coding Genes Have a .pdf:application/pdf},
