	pages = {1755--1778},
	file = {Schalon et al. - 2008 - A simple and fuzzy method to align and compare dru.pdf:/home/acomajuncosa/snap/zotero-snap/common/Zotero/storage/BXG2H9K4/Schalon et al. - 2008 - A simple and fuzzy method to align and compare dru.pdf:application/pdf},
