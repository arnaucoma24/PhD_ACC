@article{zhang_benchmarking_2023,
	title = {Benchmarking {Refined} and {Unrefined} {AlphaFold2} {Structures} for {Hit} {Discovery}},
	volume = {63},
	copyright = {https://doi.org/10.15223/policy-029},
